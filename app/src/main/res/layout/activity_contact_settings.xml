<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_contact_settings"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ashkp.mycontactlist.ContactSettingsActivity">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_contact"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.ashkp.mycontactlist.ContactActivity">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/navbar"
            android:background="@color/navbar_background"
            android:layout_alignParentBottom="true">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/contactlisticon"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@id/imageButtonMap"
                android:layout_toStartOf="@id/imageButtonMap"
                android:layout_marginRight="20dp"
                android:layout_marginEnd="20dp"
                android:id="@+id/imageButtonList" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/mapicon"
                android:id="@+id/imageButtonMap"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButtonSettings"
                app:srcCompat="@drawable/settingsicon"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:layout_toRightOf="@id/imageButtonMap"
                android:layout_toEndOf="@id/imageButtonMap"/>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>
